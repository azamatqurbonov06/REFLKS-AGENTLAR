        "class PochtaMuhiti:\n",
        "    def __init__(self, xabar):\n",
        "        self.xabar = xabar          # Kiruvchi xabar matni\n",
        "\n",
        "    def perseptsiya_ol(self):\n",
        "        return self.xabar\n",
        "\n",
        "    def harakat_qollash(self, harakat):\n",
        "        print(f\"  -> Xabar joylashtirildi: [{harakat}]\")\n",
        "\n",
        "\n",
        "class SpamAgenti:\n",
        "    def qaror_qabul_qil(self, perseptsiya):\n",
        "        xabar = perseptsiya.lower()   # Katta-kichik harf farqsiz qidirish\n",
        "        if \"yutuq\" in xabar or \"tekin\" in xabar:\n",
        "            return \"SPAM_PAPKA\"       # Spam kalit so'zlari aniqlandi\n",
        "        elif \"hisobot\" in xabar or \"ish\" in xabar:\n",
        "            return \"MUHIM_PAPKA\"      # Muhim ish xabari\n",
        "        else:\n",
        "            return \"KIRUVCHI_PAPKA\"   # Oddiy xabar\n",
        "\n",
        "\n",
        "# --- 20 ta sinov xabari ---\n",
        "xabarlar = [\n",
        "    \"Siz katta yutuq qozondiniz, sovg'angizni oling!\",\n",
        "    \"Bugungi ish hisoboti tayyor va yuborildi.\",\n",
        "    \"Salom, qanday yuribsiz? Yaxshi kunlar!\",\n",
        "    \"Tekin kurs! Ro'yxatdan o'ting hoziroq!\",\n",
        "    \"Loyiha ish rejasi haqida muhokama kerak.\",\n",
        "    \"Kechqurun uchrashamizmi, do'stim?\",\n",
        "    \"Haftalik hisobot uchun ma'lumotlar kerak.\",\n",
        "    \"Yutuq! Million dollar yutdingiz, shoshiling!\",\n",
        "    \"Ertaga ish boshlanadi, tayyor bo'ling.\",\n",
        "    \"Yangi rasm yuklandim, qarang!\",\n",
        "    \"Tekin internet topildi, bu linkni bos!\",\n",
        "    \"Xavfsizlik hisoboti tayyorlandi.\",\n",
        "    \"Dam olish kunlari qanday o'tdi?\",\n",
        "    \"Yutuqli aksiya: bepul mahsulot oling!\",\n",
        "    \"Ish joyida yangi qoidalar joriy qilindi.\",\n",
        "    \"Tug'ilgan kuningiz bilan tabriklayman!\",\n",
        "    \"Tekin taklif: bugun faqat 100 ta!\",\n",
        "    \"Moliyaviy hisobot yuborildi, tekshiring.\",\n",
        "    \"Kutubxonaga kitob qaytarishni unutmang.\",\n",
        "    \"Yutuq o'yinida qatnashing va g'olib bo'ling!\"\n",
        "]\n",
        "\n",
        "s_agent = SpamAgenti()\n",
        "\n",
        "print(\"=\" * 65)\n",
        "print(\"MASALA 8: Spam Aniqlovchi Agenti - 20 ta xabar tekshiruvi\")\n",
        "print(\"=\" * 65)\n",
        "\n",
        "for qadam, xabar in enumerate(xabarlar, 1):\n",
        "    p_muhit = PochtaMuhiti(xabar)\n",
        "    perseptsiya = p_muhit.perseptsiya_ol()\n",
        "    harakat = s_agent.qaror_qabul_qil(perseptsiya)\n",
        "    print(f\"Qadam {qadam:2d}: '{xabar[:45]} {harakat}\")\n",
        "    p_muhit.harakat_qollash(harakat)\n"
      ]
    },
