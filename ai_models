        "3. harorat <= 5 va mahsulot > 0 ‚Üí NORMAL_REJIM\n",
        "4. mahsulot_soni == 0 ‚Üí BUYURTMA_BERISH\n",
        "5. Aks holda ‚Üí NORMAL_HOLAT\n"
      ]
