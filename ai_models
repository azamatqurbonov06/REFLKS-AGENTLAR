        "class DataMarkazMuhiti:\n",
        "    def __init__(self):\n",
        "        self.cpu_yuklama = random.randint(0, 100)   # CPU foizi\n",
        "\n",
        "    def perseptsiya_ol(self):\n",
        "        return self.cpu_yuklama\n",
        "\n",
        "    def harakat_qollash(self, harakat):\n",
        "        print(f\"   Server holati: {harakat}\")\n",
        "\n",
        "\n",
        "class BalansAgenti:\n",
        "    def qaror_qabul_qil(self, perseptsiya):\n",
        "        cpu_yuklama = perseptsiya\n",
        "        if cpu_yuklama > 80:\n",
        "            return \"SERVER_QOSHISH\"    # Yuklanish yuqori ‚Äî qo'shimcha server yoq\n",
        "        elif cpu_yuklama < 20:\n",
        "            return \"SERVER_OCHIRISH\"   # Yuklanish past ‚Äî ortiqcha serverni o'chir\n",
        "        else:\n",
        "            return \"STABIL_HOLAT\"      # Barcha normal\n",
        "\n",
        "\n",
        "# --- Simulyatsiya ---\n",
        "d_muhit = DataMarkazMuhiti()\n",
        "d_agent = BalansAgenti()\n",
        "\n",
        "print(\"=\" * 60)\n",
        "print(\"MASALA 6: Server Balans Agenti - 20 qadam simulyatsiya\")\n",
        "print(\"=\" * 60)\n",
        "\n",
        "for qadam in range(1, 21):\n",
        "    d_muhit.cpu_yuklama = random.randint(0, 100)\n",
        "    perseptsiya = d_muhit.perseptsiya_ol()\n",
        "    harakat = d_agent.qaror_qabul_qil(perseptsiya)\n",
        "    print(f\"Qadam {qadam:2d}: CPU={perseptsiya:3d} Harakat: {harakat}\")\n",
        "    d_muhit.harakat_qollash(harakat)\n"
      ]
    },
