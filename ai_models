        "class GarajMuhiti:\n",
        "    def __init__(self):\n",
        "        self.masofa = round(random.uniform(0.1, 6.0), 1)   # Metrda float qiymat\n",
        "\n",
        "    def perseptsiya_ol(self):\n",
        "        return self.masofa\n",
        "\n",
        "    def harakat_qollash(self, harakat):\n",
        "        print(f\"  -> Avtomobil harakati: {harakat}\")\n",
        "\n",
        "\n",
        "class ParkovkaAgenti:\n",
        "    def qaror_qabul_qil(self, perseptsiya):\n",
        "        masofa = perseptsiya\n",
        "        if masofa <= 0.5:\n",
        "            return \"TOXTASH\"          # Xavfli zona ‚Äî to'xta!\n",
        "        elif masofa > 0.5 and masofa <= 2.0:\n",
        "            return \"SEKINLASHISH\"     # Ogohlantirish ‚Äî sekin yur\n",
        "        else:\n",
        "            return \"ORQAGA_YURISH\"    # Xavfsiz zona ‚Äî davom et\n",
        "\n",
        "\n",
        "# --- Simulyatsiya ---\n",
        "g_muhit = GarajMuhiti()\n",
        "g_agent = ParkovkaAgenti()\n",
        "\n",
        "print(\"=\" * 60)\n",
        "print(\"MASALA 9: Parkovka Agenti - 20 qadam simulyatsiya\")\n",
        "print(\"=\" * 60)\n",
        "\n",
        "for qadam in range(1, 21):\n",
        "    g_muhit.masofa = round(random.uniform(0.1, 6.0), 1)\n",
        "    perseptsiya = g_muhit.perseptsiya_ol()\n",
        "    harakat = g_agent.qaror_qabul_qil(perseptsiya)\n",
        "    print(f\"Qadam {qadam:2d}: Masofa={perseptsiya:4.1f}  Harakat: {harakat}\")\n",
        "    g_muhit.harakat_qollash(harakat)\n"
      ]
    },
