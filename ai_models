      "source": [
        "class HaroratMuhiti:\n",
        "    def __init__(self):\n",
        "        # Tasodifiy harorat 10-35 oralig'ida\n",
        "        self.harorat = random.randint(10, 35)\n",
        "\n",
        "    def perseptsiya_ol(self):\n",
        "        return self.harorat\n",
        "\n",
        "    def harakat_qollash(self, harakat):\n",
        "        print(f\"  -> Qurilma buyrug'i: {harakat}\")\n",
        "\n",
        "\n",
        "class HaroratAgenti:\n",
        "    def qaror_qabul_qil(self, perseptsiya):\n",
        "        harorat = perseptsiya\n",
        "        if harorat < 18:\n",
        "            return \"ISITISH_YOQ\"    # Sovuq ‚Äî isitgichni yoq\n",
        "        elif harorat > 26:\n",
        "            return \"SOVITISH_YOQ\"   # Issiq ‚Äî konditsionerni yoq\n",
        "        else:\n",
        "            return \"KUTISH\"         # Normal harorat ‚Äî hech narsa qilma\n",
        "\n",
        "\n",
        "# --- Simulyatsiya ---\n",
        "h_muhit = HaroratMuhiti()\n",
        "h_agent = HaroratAgenti()\n",
        "\n",
        "print(\"=\" * 55)\n",
        "print(\"MASALA 2: Harorat Agenti  20 qadam simulyatsiya\")\n",
        "print(\"=\" * 55)\n",
        "\n",
        "for qadam in range(1, 21):\n",
        "    h_muhit.harorat = random.randint(10, 35)\n",
        "    perseptsiya = h_muhit.perseptsiya_ol()\n",
        "    harakat = h_agent.qaror_qabul_qil(perseptsiya)\n",
        "    print(f\"Qadam {qadam:2d}: Harorat={perseptsiya}  Harakat: {harakat}\")\n",
        "    h_muhit.harakat_qollash(harakat)\n"
      ]
    },
